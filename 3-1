#include <math.h> 
#include <stdio.h> 

/**
 * @brief Функция расчета формулы
 * @param x значение параметра
 * @return Значение вычисления y  
*/
float function(float x);

/**
 * @brief Функция проверки ввода на число 
 * @param x значение вводимого параметра 
 * @return Результат проверки   
*/
double fun_scan();

int main () 
{ 
    float x, y, x1, x2, x3; 
    x1 = fun_scan();
    x2 = fun_scan();
    x3 = fun_scan();
    for (x=x1; x<=x2; x+=x3){
        y=function(x);
        printf ("y=%f\n x=%f\n", y, x); 
    }
}

float function(float x)
{
    float q =3*pow(log(x), 2) + 6*log(x)-5;
    return (q);
}
double fun_scan() {
    int a;
    double b;
    a=scanf("%lf", &b);
    if ((a!=1)&&(a>0)){
        printf("%s", "Error");
    } 
    return b;
}
