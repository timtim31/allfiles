#include <stdio.h>

/**
 * @brief Функция расчета формулы
 * @param x значение параметра
 * @return Значение вычислений  
*/
float function(float x);



int main()
{
    float a, r;
    scanf("%f", &a);
    
    r=function(a);
    printf("r=%f\n", r);
}

float function(float x)
{
    float y=x/2.5;
    return (y);
}
